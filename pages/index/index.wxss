/**index.wxss**/
page {
  height: 100%;
  background-color: #333333


}
.container {
  display: flex;
  width: 100%;
  min-height: 100%;
  flex-wrap: wrap
}

.item{
  flex: 0 0 50%;
  height: 360rpx;
}
.navigator {
  width: 90%;
  height: 90%;
  padding: 5%;
  position: relative;
  text-align: right
}
.cartBorder{
  width: 50rpx;
  height: 50rpx;
  position: fixed;
  z-index: 1000;
  background-color: red;
  border-radius: 50%;
  right: 80rpx;
  top: 900rpx;
  opacity: 0.6
}
.shopCart {
  
}
._shopCart {
  width: 50rpx;
  height: 50rpx;
  z-index: 100;
  position: absolute;
  margin-top: 8rpx;
  right: 30rpx

}

image {
  width: 100%;
  height: 70%;
  display: block
}
.detail {
  height: 30%;
  background: white;
  font-size: 30rpx;
  font-family:"微软雅黑";
  text-align: left;
  padding-left: 10rpx;
}
.price {
  color: red;
  font-size: 20rpx;
  text-align: center
}

